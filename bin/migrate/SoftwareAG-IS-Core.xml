<ehcache     
    name="SoftwareAG.IS.Core"     
    updateCheck="false"     
    monitoring="on">    
    <diskStore         
        path="./cacheStore/SoftwareAG-IS-Core"/>    
    <cacheManagerEventListenerFactory         
        class=""         
        properties=""/>    
    <defaultCache         
        maxEntriesLocalHeap="0">        
        <elementValueComparator             
            class="net.sf.ehcache.store.DefaultElementValueComparator"/>    
    </defaultCache>    
    <cache         
        name="OAuthAuthCodes"         
        eternal="true"         
        maxEntriesLocalHeap="1000"         
        maxEntriesLocalDisk="1000"         
        statistics="true">        
        <persistence             
            strategy="none"/>        
        <elementValueComparator             
            class="net.sf.ehcache.store.DefaultElementValueComparator"/>        
        <terracotta             
            clustered="false"             
            consistency="STRONG">            
            <nonstop                 
                immediateTimeout="true"                 
                timeoutMillis="45000"/>        
        </terracotta>    
    </cache>    
    <cache         
        name="ClusterMembers"         
        maxEntriesLocalHeap="1000"         
        timeToLiveSeconds="125"         
        maxEntriesLocalDisk="1000"         
        statistics="true">        
        <persistence             
            strategy="none"/>        
        <elementValueComparator             
            class="net.sf.ehcache.store.DefaultElementValueComparator"/>        
        <terracotta             
