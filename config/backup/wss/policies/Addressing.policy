  <!--
    This WS-Addressing policy expects messages to confirm to WS-Addressing Final specification 
  -->
  
<wsp:Policy xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" 
xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" 
xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" Name="Addressing" wsu:Id="Addressing">
        <wsp:ExactlyOne>
            <wsp:All>
                <wsaw:UsingAddressing xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl" wsdl:required="true"/>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>