<?xml version="1.0" encoding="UTF-8"?>

<StorageConfig>
	<database>
		<driver-class>org.apache.derby.jdbc.EmbeddedDriver</driver-class>
		<pool>jdbc:derby:AUDITINGSC;create=true</pool>
		<storeid>AuditStore</storeid>
		<queue-table>AUDIT_QUEUES</queue-table>
	</database>
	<Monitor>
		<NumberOfMessagesSinceStartup>off</NumberOfMessagesSinceStartup>
		<NumberOfRedeliveryMessagesSinceStartup>off</NumberOfRedeliveryMessagesSinceStartup>
		<AverageDeliveryCount>off</AverageDeliveryCount>
		<MessageAverageLifeSpan>off</MessageAverageLifeSpan>
		<LastEnqueueTimestamp>off</LastEnqueueTimestamp>
		<LastDequeueTimestamp>off</LastDequeueTimestamp>
	</Monitor>
</StorageConfig>
